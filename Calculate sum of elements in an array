#include <stdio.h>
int main() 
{
    int i,size,sum=0,a[size];
    printf("Enter size of the array : ");
    scanf("%d",&size); 
    printf("Enter elements in array : ");
    for(i=0; i<size; i++) 
    {
        scanf("%d",&a[i]);
    }
for(i=0; i<size; i++) 
{
sum+=a[i];
}
printf("sum of array is : %d",sum);
}
